        <rdfs:range rdf:resource="http://www.semanticweb.org/talliente/abbigliamento#Tessuto"/>
        <rdfs:comment xml:lang="en">relationship that binds clothing to the fabric it is made of</rdfs:comment>
        <rdfs:comment xml:lang="it">relazione che lega l&apos;abbigliamento al tessuto di cui è fatto</rdfs:comment>
        <rdfs:label xml:lang="it">fatto di</rdfs:label>
        <rdfs:label xml:lang="en">made of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/talliente/abbigliamento#haAbbigliamento -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/talliente/abbigliamento#haAbbigliamento">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/talliente/abbigliamento#indossatoIn"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/talliente/abbigliamento#Stagione"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/talliente/abbigliamento#Abbigliamento"/>
        <rdfs:comment xml:lang="en">relationship that binds a season to clothing</rdfs:comment>
        <rdfs:comment xml:lang="it">relazione che lega una stagione all&apos;abbigliamento</rdfs:comment>
        <rdfs:label xml:lang="it">ha abbigliamento</rdfs:label>
        <rdfs:label xml:lang="en">has clothing</rdfs:label>
    </owl:ObjectProperty>
    


