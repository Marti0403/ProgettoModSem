    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/talliente/abbigliamento#progettatoDa">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/talliente/abbigliamento#Abbigliamento"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/talliente/abbigliamento#Stilista"/>
        <rdfs:comment xml:lang="en">relationship indicating clothing designed by a stylist</rdfs:comment>
        <rdfs:comment xml:lang="it">relazione che indica l&apos;abbigliamento progettato da uno stilista</rdfs:comment>
        <rdfs:label xml:lang="en">dedsigns by</rdfs:label>
        <rdfs:label xml:lang="it">progettato da</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/talliente/abbigliamento#tessutoDi -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/talliente/abbigliamento#tessutoDi">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/talliente/abbigliamento#Tessuto"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/talliente/abbigliamento#Abbigliamento"/>
