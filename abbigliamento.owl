        <rdfs:domain rdf:resource="http://www.semanticweb.org/talliente/abbigliamento#TessutoPuro"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/talliente/abbigliamento#Stagione"/>
        <rdfs:comment xml:lang="en">relationship that binds the pure fabric ideal for one season to the season</rdfs:comment>
        <rdfs:comment xml:lang="it">relazione che lega il tessuto puro ideale per una stagione alla stagione</rdfs:comment>
        <rdfs:label xml:lang="en">ideal for</rdfs:label>
        <rdfs:label xml:lang="it">ideale per</rdfs:label>
    </owl:ObjectProperty>
    


