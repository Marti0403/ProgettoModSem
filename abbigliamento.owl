        <rdfs:comment xml:lang="it">relazione che indica quale tessuto è usato per creare un abbigliamento</rdfs:comment>
        <rdfs:label xml:lang="en">fabric of</rdfs:label>
        <rdfs:label xml:lang="it">tessuto di</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/talliente/abbigliamento#vende -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/talliente/abbigliamento#vende">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/talliente/abbigliamento#vendutoDa"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/talliente/abbigliamento#Venditore"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/talliente/abbigliamento#Abbigliamento"/>
        <rdfs:comment xml:lang="en">relationship that indicates the vendor selling clothing</rdfs:comment>
        <rdfs:comment xml:lang="it">relazione che indica il venditore che vende un abbigliamento</rdfs:comment>
        <rdfs:label xml:lang="en">sells</rdfs:label>
        <rdfs:label xml:lang="it">vende</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/talliente/abbigliamento#vendutoDa -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/talliente/abbigliamento#vendutoDa">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/talliente/abbigliamento#Abbigliamento"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/talliente/abbigliamento#Venditore"/>
        <rdfs:comment xml:lang="en">relationship that links the clothing sold by a seller to the seller</rdfs:comment>
        <rdfs:comment xml:lang="it">relazione che lega l&apos;abbigliamento venduto da un venditore al venditore</rdfs:comment>
        <rdfs:label xml:lang="en">sells by</rdfs:label>
        <rdfs:label xml:lang="it">venduto da</rdfs:label>
    </owl:ObjectProperty>
    


