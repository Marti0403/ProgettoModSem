        <rdfs:domain rdf:resource="http://www.semanticweb.org/talliente/abbigliamento#Materiale"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/talliente/abbigliamento#Tessuto"/>
        <rdfs:comment xml:lang="en">relationship that binds the material used in the composition of a fabric to the fabric</rdfs:comment>
        <rdfs:comment xml:lang="it">relazione che lega il materiale usato nella composizione di un tessuto al tessuto</rdfs:comment>
        <rdfs:label xml:lang="it">compone</rdfs:label>
        <rdfs:label xml:lang="en">composes</rdfs:label>
    </owl:ObjectProperty>
    


