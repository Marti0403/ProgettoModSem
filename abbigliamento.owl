    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/talliente/abbigliamento#codiceIdentificativo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/talliente/abbigliamento#Abbigliamento"/>
