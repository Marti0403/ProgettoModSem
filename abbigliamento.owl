        <rdfs:comment xml:lang="it">Relazione usata per abbinare i capi di abbigliamento</rdfs:comment>
        <rdfs:label xml:lang="it">abbinato con</rdfs:label>
        <rdfs:label xml:lang="en">matched with</rdfs:label>
    </owl:ObjectProperty>
    


