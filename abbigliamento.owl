        <rdfs:comment xml:lang="it">costo di un tessuto al metro in euro</rdfs:comment>
        <rdfs:label xml:lang="en">cost</rdfs:label>
        <rdfs:label xml:lang="it">costo</rdfs:label>
    </owl:DatatypeProperty>
    


