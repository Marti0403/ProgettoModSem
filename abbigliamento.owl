    <!-- http://www.semanticweb.org/talliente/abbigliamento#abbinatoCon -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/talliente/abbigliamento#abbinatoCon">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/talliente/abbigliamento#Abbigliamento"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/talliente/abbigliamento#Abbigliamento"/>
