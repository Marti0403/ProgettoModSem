        <rdfs:range rdf:resource="http://www.semanticweb.org/talliente/abbigliamento#Materiale"/>
        <rdfs:comment xml:lang="en">relationship that binds a fabric to the material it is made of</rdfs:comment>
        <rdfs:comment xml:lang="it">relazione che lega un tessuto al materiale di cui è composto</rdfs:comment>
        <rdfs:label xml:lang="en">composed of</rdfs:label>
        <rdfs:label xml:lang="it">composto da</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/talliente/abbigliamento#creatoDa -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/talliente/abbigliamento#creatoDa">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/talliente/abbigliamento#Abbigliamento"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/agentrole.owl#Agent"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.org/talliente/abbigliamento#progettatoDa"/>
            <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#isRoleOf"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="en">relationship indicating which clothing is created by an agent</rdfs:comment>
        <rdfs:comment xml:lang="it">relazione che indica quale abbigliamento è creato da un agente</rdfs:comment>
        <rdfs:label xml:lang="en">created by</rdfs:label>
        <rdfs:label xml:lang="it">creato da</rdfs:label>
    </owl:ObjectProperty>
    


