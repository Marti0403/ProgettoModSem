        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/talliente/abbigliamento#Abbigliamento"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/talliente/abbigliamento#Materiale"/>
        <rdfs:comment xml:lang="en">relationship that binds the clothing to the material, in particular to the main material used in its creation</rdfs:comment>
        <rdfs:comment xml:lang="it">relazione che lega l&apos;abbigliamento al materiale, in particolare al materiale principale usato nella sua realizzazione</rdfs:comment>
        <rdfs:label xml:lang="it">ha materiale principale</rdfs:label>
        <rdfs:label xml:lang="en">has main material</rdfs:label>
    </owl:ObjectProperty>
    


