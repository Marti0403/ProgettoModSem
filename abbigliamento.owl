    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/talliente/abbigliamento#indossatoIn">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/talliente/abbigliamento#Abbigliamento"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/talliente/abbigliamento#Stagione"/>
        <rdfs:comment xml:lang="en">relationship that binds clothing in the season in which it is worn</rdfs:comment>
        <rdfs:comment xml:lang="it">relazione che lega l&apos;abbigliamento nella stagione in cui è indossato</rdfs:comment>
        <rdfs:label xml:lang="it">indossato in</rdfs:label>
        <rdfs:label xml:lang="en">worn in</rdfs:label>
    </owl:ObjectProperty>
    


