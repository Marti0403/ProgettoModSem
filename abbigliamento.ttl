#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( <http://www.ontologydesignpatterns.org/cp/owl/classification.owl#Concept>
                objectrole:Object
                abbigliamento:Abbigliamento
                abbigliamento:Materiale
                abbigliamento:Tessuto
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( abbigliamento:MaterialeNaturaleAnimale
                abbigliamento:MaterialeNaturaleMinerale
                abbigliamento:MaterialeNaturaleVegetale
              )
] .


