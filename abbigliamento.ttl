@prefix : <http://www.semanticweb.org/talliente/abbigliamento#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix objectrole: <http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#> .
@prefix abbigliamento: <http://www.semanticweb.org/talliente/abbigliamento#> .
@prefix cpannotationschema: <http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#> .
@base <http://www.semanticweb.org/talliente/abbigliamento> .

<http://www.semanticweb.org/talliente/abbigliamento> rdf:type owl:Ontology ;
                                                      owl:imports <http://www.ontologydesignpatterns.org/cp/owl/agentrole.owl> ;
                                                      <http://purl.org/dc/elements/1.1/creator> "Martina Talliente"@it ;
                                                      <http://purl.org/dc/elements/1.1/date> "2022-05-11"@it ;
                                                      <http://purl.org/dc/elements/1.1/description> "Ontologia dedicata all'abbigliamento, tessuti, materiali che lo compongono, alle persone che contribuiscono alla sua produzione e vendita, alle stagioni a cui è associato."@it ;
                                                      <http://purl.org/dc/elements/1.1/title> "Abbigliamento 2022"@it ;
                                                      <http://purl.org/vocab/vann/preferredNamespacePrefix> "a22"@it ,
                                                                                                            "http://www.semanticweb.org/talliente/abbigliamento#"@it ;
                                                      rdfs:label "Abbigliamento"@it .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
<http://purl.org/dc/elements/1.1/date> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
<http://purl.org/dc/elements/1.1/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
<http://purl.org/dc/elements/1.1/title> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
<http://purl.org/vocab/vann/preferredNamespacePrefix> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
<http://purl.org/vocab/vann/preferredNamespaceUri> rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


###  http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#hasRole
objectrole:hasRole rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#isRoleOf
objectrole:isRoleOf rdf:type owl:ObjectProperty ,
                             owl:FunctionalProperty .


###  http://www.semanticweb.org/talliente/abbigliamento#abbinatoCon
abbigliamento:abbinatoCon rdf:type owl:ObjectProperty ,
                                   owl:SymmetricProperty ;
                          rdfs:domain abbigliamento:Abbigliamento ;
                          rdfs:range abbigliamento:Abbigliamento ;
                          rdfs:comment "Relationship used to match clothing"@en ,
                                       "Relazione usata per abbinare i capi di abbigliamento"@it ;
                          rdfs:label "abbinato con"@it ,
                                     "matched with"@en .


###  http://www.semanticweb.org/talliente/abbigliamento#associato
abbigliamento:associato rdf:type owl:ObjectProperty ;
                        rdfs:domain abbigliamento:Tessuto ;
                        rdfs:range abbigliamento:Stagione ;
                        rdfs:comment "relationship that associates a fabric with a season"@en ,
                                     "relazione che associa un tessuto a una stagione"@it ;
                        rdfs:label "associated"@en ,
                                   "associato"@it .


###  http://www.semanticweb.org/talliente/abbigliamento#compone
abbigliamento:compone rdf:type owl:ObjectProperty ;
                      owl:inverseOf abbigliamento:compostoDa ;
                      rdfs:domain abbigliamento:Materiale ;
                      rdfs:range abbigliamento:Tessuto ;
                      rdfs:comment "relationship that binds the material used in the composition of a fabric to the fabric"@en ,
                                   "relazione che lega il materiale usato nella composizione di un tessuto al tessuto"@it ;
                      rdfs:label "compone"@it ,
                                 "composes"@en .


###  http://www.semanticweb.org/talliente/abbigliamento#compostoDa
abbigliamento:compostoDa rdf:type owl:ObjectProperty ;
                         rdfs:domain abbigliamento:Tessuto ;
                         rdfs:range abbigliamento:Materiale ;
                         rdfs:comment "relationship that binds a fabric to the material it is made of"@en ,
                                      "relazione che lega un tessuto al materiale di cui è composto"@it ;
                         rdfs:label "composed of"@en ,
                                    "composto da"@it .


###  http://www.semanticweb.org/talliente/abbigliamento#creatoDa
abbigliamento:creatoDa rdf:type owl:ObjectProperty ;
                       rdfs:domain abbigliamento:Abbigliamento ;
                       rdfs:range <http://www.ontologydesignpatterns.org/cp/owl/agentrole.owl#Agent> ;
                       owl:propertyChainAxiom ( abbigliamento:progettatoDa
                                                objectrole:isRoleOf
                                              ) ;
                       rdfs:comment "relationship indicating which clothing is created by an agent"@en ,
                                    "relazione che indica quale abbigliamento è creato da un agente"@it ;
                       rdfs:label "created by"@en ,
                                  "creato da"@it .


###  http://www.semanticweb.org/talliente/abbigliamento#fattoDi
abbigliamento:fattoDi rdf:type owl:ObjectProperty ;
                      owl:inverseOf abbigliamento:tessutoDi ;
                      rdf:type owl:FunctionalProperty ;
                      rdfs:domain abbigliamento:Abbigliamento ;
                      rdfs:range abbigliamento:Tessuto ;
                      rdfs:comment "relationship that binds clothing to the fabric it is made of"@en ,
                                   "relazione che lega l'abbigliamento al tessuto di cui è fatto"@it ;
                      rdfs:label "fatto di"@it ,
                                 "made of"@en .


###  http://www.semanticweb.org/talliente/abbigliamento#haAbbigliamento
abbigliamento:haAbbigliamento rdf:type owl:ObjectProperty ;
                              owl:inverseOf abbigliamento:indossatoIn ;
                              rdfs:domain abbigliamento:Stagione ;
                              rdfs:range abbigliamento:Abbigliamento ;
                              rdfs:comment "relationship that binds a season to clothing"@en ,
                                           "relazione che lega una stagione all'abbigliamento"@it ;
                              rdfs:label "ha abbigliamento"@it ,
                                         "has clothing"@en .


###  http://www.semanticweb.org/talliente/abbigliamento#haMaterialePrincipale
abbigliamento:haMaterialePrincipale rdf:type owl:ObjectProperty ,
                                             owl:FunctionalProperty ;
                                    rdfs:domain abbigliamento:Abbigliamento ;
                                    rdfs:range abbigliamento:Materiale ;
                                    rdfs:comment "relationship that binds the clothing to the material, in particular to the main material used in its creation"@en ,
                                                 "relazione che lega l'abbigliamento al materiale, in particolare al materiale principale usato nella sua realizzazione"@it ;
                                    rdfs:label "ha materiale principale"@it ,
                                               "has main material"@en .


###  http://www.semanticweb.org/talliente/abbigliamento#idealePer
abbigliamento:idealePer rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf abbigliamento:associato ;
                        rdfs:domain abbigliamento:TessutoPuro ;
                        rdfs:range abbigliamento:Stagione ;
                        rdfs:comment "relationship that binds the pure fabric ideal for one season to the season"@en ,
                                     "relazione che lega il tessuto puro ideale per una stagione alla stagione"@it ;
                        rdfs:label "ideal for"@en ,
                                   "ideale per"@it .


###  http://www.semanticweb.org/talliente/abbigliamento#indossatoIn
abbigliamento:indossatoIn rdf:type owl:ObjectProperty ;
                          rdfs:domain abbigliamento:Abbigliamento ;
                          rdfs:range abbigliamento:Stagione ;
                          rdfs:comment "relationship that binds clothing in the season in which it is worn"@en ,
                                       "relazione che lega l'abbigliamento nella stagione in cui è indossato"@it ;
                          rdfs:label "indossato in"@it ,
                                     "worn in"@en .


###  http://www.semanticweb.org/talliente/abbigliamento#progetta
abbigliamento:progetta rdf:type owl:ObjectProperty ;
                       owl:inverseOf abbigliamento:progettatoDa ;
                       rdfs:domain abbigliamento:Stilista ;
                       rdfs:range abbigliamento:Abbigliamento ;
                       rdfs:comment "relationship indicating which stylist designs clothing"@en ,
                                    "relazione che indica quale stilista progetta un abbigliamento"@it ;
                       rdfs:label "designs"@en ,
                                  "progetta"@it .


###  http://www.semanticweb.org/talliente/abbigliamento#progettatoDa
abbigliamento:progettatoDa rdf:type owl:ObjectProperty ;
                           rdfs:domain abbigliamento:Abbigliamento ;
                           rdfs:range abbigliamento:Stilista ;
                           rdfs:comment "relationship indicating clothing designed by a stylist"@en ,
                                        "relazione che indica l'abbigliamento progettato da uno stilista"@it ;
                           rdfs:label "dedsigns by"@en ,
                                      "progettato da"@it .


###  http://www.semanticweb.org/talliente/abbigliamento#tessutoDi
abbigliamento:tessutoDi rdf:type owl:ObjectProperty ;
                        rdfs:domain abbigliamento:Tessuto ;
                        rdfs:range abbigliamento:Abbigliamento ;
                        rdfs:comment "relationship indicating which fabric is used to create a clothing"@en ,
                                     "relazione che indica quale tessuto è usato per creare un abbigliamento"@it ;
                        rdfs:label "fabric of"@en ,
                                   "tessuto di"@it .


###  http://www.semanticweb.org/talliente/abbigliamento#vende
abbigliamento:vende rdf:type owl:ObjectProperty ;
                    owl:inverseOf abbigliamento:vendutoDa ;
                    rdfs:domain abbigliamento:Venditore ;
                    rdfs:range abbigliamento:Abbigliamento ;
                    rdfs:comment "relationship that indicates the vendor selling clothing"@en ,
                                 "relazione che indica il venditore che vende un abbigliamento"@it ;
                    rdfs:label "sells"@en ,
                               "vende"@it .


###  http://www.semanticweb.org/talliente/abbigliamento#vendutoDa
abbigliamento:vendutoDa rdf:type owl:ObjectProperty ;
                        rdfs:domain abbigliamento:Abbigliamento ;
                        rdfs:range abbigliamento:Venditore ;
                        rdfs:comment "relationship that links the clothing sold by a seller to the seller"@en ,
                                     "relazione che lega l'abbigliamento venduto da un venditore al venditore"@it ;
                        rdfs:label "sells by"@en ,
                                   "venduto da"@it .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/talliente/abbigliamento#ComposizioneMaterialePrincipale
abbigliamento:ComposizioneMaterialePrincipale rdf:type owl:DatatypeProperty ;
                                              rdfs:domain abbigliamento:Abbigliamento ;
                                              rdfs:range xsd:int ;
                                              rdfs:comment "la composizione in percentuale del materiale principale usato nel tessuto di un abbigliamento"@it ,
                                                           "the percentage composition of the material used in the fabric of a clothing"@en ;
                                              rdfs:label "composizione materiale principale"@it ,
                                                         "main material composition"@en .


###  http://www.semanticweb.org/talliente/abbigliamento#codiceIdentificativo
abbigliamento:codiceIdentificativo rdf:type owl:DatatypeProperty ,
                                            owl:FunctionalProperty ;
                                   rdfs:domain abbigliamento:Abbigliamento ;
                                   rdfs:comment "Unique code to identificate the item of clothing"@en ,
                                                "codice univoco per identificare il capo di abbigliamento"@it ;
                                   rdfs:label "code"@en ,
                                              "codice"@it .


###  http://www.semanticweb.org/talliente/abbigliamento#costo
abbigliamento:costo rdf:type owl:DatatypeProperty ;
                    rdfs:domain abbigliamento:Tessuto ;
                    rdfs:range xsd:integer ;
                    rdfs:comment "cost per meter of a fabric in euro"@en ,
                                 "costo di un tessuto al metro in euro"@it ;
                    rdfs:label "cost"@en ,
                               "costo"@it .


###  http://www.semanticweb.org/talliente/abbigliamento#provenienza
abbigliamento:provenienza rdf:type owl:DatatypeProperty ;
                          rdfs:domain abbigliamento:Tessuto ;
                          rdfs:range xsd:string ;
                          rdfs:comment "country of origin of the fabric"@en ,
                                       "paese di provenienza del tessuto"@it ;
                          rdfs:label "origin"@en ,
                                     "provenienza"@it .


#################################################################
#    Classes
#################################################################

###  http://www.ebusiness-unibw.org/ontologies/cpi/ns#cpi:ClothingAndAccessories
<http://www.ebusiness-unibw.org/ontologies/cpi/ns#cpi:ClothingAndAccessories> rdf:type owl:Class ;
                                                                              owl:equivalentClass abbigliamento:Abbigliamento .


###  http://www.ontologydesignpatterns.org/cp/owl/agentrole.owl#Agent
<http://www.ontologydesignpatterns.org/cp/owl/agentrole.owl#Agent> rdf:type owl:Class ;
                                                                   owl:equivalentClass <http://xmlns.com/foaf/0.1/Agent> ;
                                                                   rdfs:subClassOf objectrole:Object .


###  http://www.ontologydesignpatterns.org/cp/owl/classification.owl#Concept
<http://www.ontologydesignpatterns.org/cp/owl/classification.owl#Concept> rdf:type owl:Class .


###  http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#Object
objectrole:Object rdf:type owl:Class .


###  http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#Role
objectrole:Role rdf:type owl:Class ;
                rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/classification.owl#Concept> .


###  http://www.semanticweb.org/talliente/abbigliamento#Abbigliamento
abbigliamento:Abbigliamento rdf:type owl:Class ;
                            rdfs:comment "Capo di abbigliamento con cui ci si veste"@it ,
                                         "Item of clothing with which you dress"@en ;
                            rdfs:label "Abbigliamento"@it ,
                                       "Clothing"@en .


###  http://www.semanticweb.org/talliente/abbigliamento#AbbigliamentoDiQualità
abbigliamento:AbbigliamentoDiQualità rdf:type owl:Class ;
                                     rdfs:subClassOf abbigliamento:Abbigliamento ;
                                     rdfs:comment "Capo di abbigliamento ritenuto di qualità"@it ,
                                                  "item of clothing condìsidered to be of quality"@en ;
                                     rdfs:label "Abbigliamento di qualità"@it ,
                                                "Quality clothing"@en .


###  http://www.semanticweb.org/talliente/abbigliamento#AbbigliamentoIntelligente
abbigliamento:AbbigliamentoIntelligente rdf:type owl:Class ;
                                        owl:equivalentClass [ rdf:type owl:Restriction ;
                                                              owl:onProperty abbigliamento:fattoDi ;
                                                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                              owl:onClass abbigliamento:TessutoElettronico
                                                            ] ;
                                        rdfs:subClassOf abbigliamento:Abbigliamento ;
                                        rdfs:label "Abbigliamento intelligente"@it .


###  http://www.semanticweb.org/talliente/abbigliamento#Materiale
abbigliamento:Materiale rdf:type owl:Class ;
                        owl:disjointWith abbigliamento:Tessuto ;
                        rdfs:comment "Sostanza usata per produrre tessuti"@it ,
                                     "Substance used to produce fabrics"@en ;
                        rdfs:label "Material"@en ,
                                   "Materiale"@it .


###  http://www.semanticweb.org/talliente/abbigliamento#MaterialeChimico
abbigliamento:MaterialeChimico rdf:type owl:Class ;
                               rdfs:subClassOf abbigliamento:Materiale ;
                               owl:disjointWith abbigliamento:MaterialeNaturale ;
                               rdfs:comment "Material that doesn't exist in nature, invented by man through chemical processes"@en ,
                                            "Materiale non esistente in natura, inventato dall'uomo tramite processi chimici"@it ;
                               rdfs:label "Chemical material"@en ,
                                          "Materiale chimico"@it .


###  http://www.semanticweb.org/talliente/abbigliamento#MaterialeNaturale
abbigliamento:MaterialeNaturale rdf:type owl:Class ;
                                rdfs:subClassOf abbigliamento:Materiale ;
                                rdfs:comment "Material of natural origin, that is, found in nature"@en ,
                                             "Materiale di origine naturale, cioè che si trova in natura"@it ;
                                rdfs:label "Materiale naturale"@it ,
                                           "Natural material"@en .


###  http://www.semanticweb.org/talliente/abbigliamento#MaterialeNaturaleAnimale
abbigliamento:MaterialeNaturaleAnimale rdf:type owl:Class ;
                                       rdfs:subClassOf abbigliamento:MaterialeNaturale ;
                                       rdfs:comment "Material found in nature and of animal origin"@en ,
                                                    "Materiale che si trova in natura e di origine animale"@it ;
                                       rdfs:label "Animal natural material"@en ,
                                                  "Materiale naturale animale"@it .


###  http://www.semanticweb.org/talliente/abbigliamento#MaterialeNaturaleMinerale
abbigliamento:MaterialeNaturaleMinerale rdf:type owl:Class ;
                                        rdfs:subClassOf abbigliamento:MaterialeNaturale ;
                                        rdfs:comment "Material found in nature and of mineral origin"@en ;
                                        rdfs:label "Materiale naturale minerale"@it ,
                                                   "Mineral natural material"@en .


###  http://www.semanticweb.org/talliente/abbigliamento#MaterialeNaturaleVegetale
abbigliamento:MaterialeNaturaleVegetale rdf:type owl:Class ;
                                        rdfs:subClassOf abbigliamento:MaterialeNaturale ;
                                        rdfs:comment "Material found in nature and of plant origin"@en ,
                                                     "Materiale che si trova in natura e di origine vegetale"@it ;
                                        rdfs:label "Materiale naturale vegetale"@it ,
                                                   "Natural plant material"@en .


###  http://www.semanticweb.org/talliente/abbigliamento#Stagione
abbigliamento:Stagione rdf:type owl:Class ;
                       owl:equivalentClass [ rdf:type owl:Class ;
                                             owl:oneOf ( abbigliamento:autunno
                                                         abbigliamento:estate
                                                         abbigliamento:inverno
                                                         abbigliamento:primavera
                                                       )
                                           ] ;
                       rdfs:subClassOf <https://www.w3.org/TR/owl-time/#time:Interval> .


###  http://www.semanticweb.org/talliente/abbigliamento#Stilista
abbigliamento:Stilista rdf:type owl:Class ;
                       owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                    owl:onProperty objectrole:isRoleOf ;
                                                                    owl:someValuesFrom <http://www.ontologydesignpatterns.org/cp/owl/agentrole.owl#Agent>
                                                                  ]
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty abbigliamento:progetta ;
                                                                    owl:someValuesFrom abbigliamento:Abbigliamento
                                                                  ]
                                                                ) ;
                                             rdf:type owl:Class
                                           ] ;
                       rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/classification.owl#Concept> ;
                       owl:disjointWith abbigliamento:Venditore ;
                       rdfs:comment "A Concept that classifies an Object as a stylist"@en ,
                                    "Un concetto che classifica un oggetto come uno stilista"@it ;
                       rdfs:label "Stilista"@it ,
                                  "Stylist"@en .


###  http://www.semanticweb.org/talliente/abbigliamento#Tessuto
abbigliamento:Tessuto rdf:type owl:Class ;
                      rdfs:comment "Tessuto tessile è un prodotto finito che si ottiene dall'intreccio di fibre tessili di uno o più materiali (cotone, seta ...)"@it ,
                                   "Textile fabric is a finished product obtained from the intertwining of textile fibers of one or more materials (cotton, silk ...)"@en ;
                      rdfs:label "Fabric"@en ,
                                 "Tessuto"@it .


###  http://www.semanticweb.org/talliente/abbigliamento#TessutoElettronico
abbigliamento:TessutoElettronico rdf:type owl:Class ;
                                 owl:equivalentClass [ rdf:type owl:Restriction ;
                                                       owl:onProperty abbigliamento:compostoDa ;
                                                       owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                       owl:onClass abbigliamento:MaterialeNaturaleMinerale
                                                     ] ;
                                 rdfs:subClassOf abbigliamento:TessutoMisto ;
                                 rdfs:comment "Fabric capable of capturing and analyzing a signal to respond appropriately"@en ,
                                              "Tessuto in grado di catturare e analizzare un segnale di rispondere in modo adeguato"@it ;
                                 rdfs:label "Eletronic fabric"@en ,
                                            "Tessuto elettronico"@it .


###  http://www.semanticweb.org/talliente/abbigliamento#TessutoMisto
abbigliamento:TessutoMisto rdf:type owl:Class ;
                           owl:equivalentClass [ rdf:type owl:Restriction ;
                                                 owl:onProperty abbigliamento:compostoDa ;
                                                 owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                                 owl:onClass abbigliamento:Materiale
                                               ] ;
                           rdfs:subClassOf abbigliamento:Tessuto ;
                           owl:disjointWith abbigliamento:TessutoPuro ;
                           rdfs:comment "Tessuto formato dall'intreccio di fibre di più materiali"@it ,
                                        "fabric formed by the intertwining of fibers of several materials"@en ;
                           rdfs:label "Mixed fabric"@en ,
                                      "Tessuto misto"@it .


###  http://www.semanticweb.org/talliente/abbigliamento#TessutoPuro
abbigliamento:TessutoPuro rdf:type owl:Class ;
                          owl:equivalentClass [ rdf:type owl:Restriction ;
                                                owl:onProperty abbigliamento:compostoDa ;
                                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                owl:onClass abbigliamento:Materiale
                                              ] ;
                          rdfs:subClassOf abbigliamento:Tessuto ;
                          rdfs:comment "Fabric formed by the intertwining of fibers of a single material"@en ,
                                       "Tessuto formato dall'intreccio di fibre di un unico materiale"@it ;
                          rdfs:label "Pure fabric"@en ,
                                     "Tessuto puro"@it .


###  http://www.semanticweb.org/talliente/abbigliamento#Venditore
abbigliamento:Venditore rdf:type owl:Class ;
                        owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                     owl:onProperty objectrole:isRoleOf ;
                                                                     owl:someValuesFrom <http://www.ontologydesignpatterns.org/cp/owl/agentrole.owl#Agent>
                                                                   ]
                                                                   [ rdf:type owl:Restriction ;
                                                                     owl:onProperty abbigliamento:vende ;
                                                                     owl:someValuesFrom abbigliamento:Abbigliamento
                                                                   ]
                                                                 ) ;
                                              rdf:type owl:Class
                                            ] ;
                        rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/classification.owl#Concept> ;
                        rdfs:comment "A Concept that classifies an Object as a vendor"@en ,
                                     "Un concetto che classifica un oggetto come un venditore"@it ;
                        rdfs:label "Venditore"@it ,
                                   "Vendor"@en .


###  http://xmlns.com/foaf/0.1/Agent
<http://xmlns.com/foaf/0.1/Agent> rdf:type owl:Class .


###  https://www.w3.org/TR/owl-time/#time:Interval
<https://www.w3.org/TR/owl-time/#time:Interval> rdf:type owl:Class .


#################################################################
#    Individuals
#################################################################

###  http://www.semanticweb.org/talliente/abbigliamento#acrilico
abbigliamento:acrilico rdf:type owl:NamedIndividual ,
                                abbigliamento:MaterialeChimico ;
                       rdfs:comment "Acrilico è un termine generico, usato per indicare una famiglia di fibre sintetiche ottenute dalla polimerizzazione dell'acrilonitrile. In particolare, affinché si possa parlare di acrilico è necessario che almeno l'85% in peso dei monomeri presenti nella catena polimerica sia costituito da acrilonitrile"@it ,
                                    "Acrylic is a generic term used to indicate a family of synthetic fibers obtained from the polymerization of acrylonitrile. In particular, in order to be able to speak of acrylic it is necessary that at least 85% by weight of the monomers present in the polymer chain is made up of acrylonitrile"@en ;
                       rdfs:label "acrilico"@it ,
                                  "acrylic"@en .


###  http://www.semanticweb.org/talliente/abbigliamento#armaniStore
abbigliamento:armaniStore rdf:type owl:NamedIndividual ,
